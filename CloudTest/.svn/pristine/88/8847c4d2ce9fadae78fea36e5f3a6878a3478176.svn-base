
h4{
	text-align: left;
	width: 100%;
/*	font-weight: 900;*/
	color: #888888;
    font-size: 15px;
	padding-left: 10px;
	margin-top: 20px;
	text-align: center;
}

.charts_bottom
{
	background: white;
    height: 45px;
	border-bottom: 1px solid #BCBAB6;
	border-top: 1px solid #BCBAB6;
	overflow: hidden;
	position: relative;
}

.charts_bottom .grade
{
	float: left;
	width: 50%;
	text-align: center;
	padding-top: 11px;
}

.charts_bottom .grade img
{
	width: 20px;
}

.charts_bottom .line
{
	position: absolute;
	 left: 50%;
	top: 10px;
	bottom: 10px;
	background: #BCBAB6;
	height: 25px;
	width: 1px;
}
